{
  "name": "theme-workspace",
  "version": "1.2.0",
  "workspaces": [
    "theme"
  ],
  "private": false,
  "description": "Mil Textos NextJS Theme",
  "author": "Mil Textos <general@miltextos.com>",
  "keywords": [
    "nextjs workspace",
    "workspace engine",
    "room workspace engine",
    "nextjs",
    "nextjs plugin",
